{
  "plugins": ["import"],
  "extends": ["airbnb-typescript", "prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/quotes": [
      "error",
      "double",
      { "allowTemplateLiterals": true }
    ],
    "@typescript-eslint/comma-dangle": "off",
    "react/jsx-filename-extension": "off"
  },
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  }
}
